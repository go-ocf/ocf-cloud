{
  "extends": [
    "config:base",
    ":preserveSemverRanges"
  ],
  "postUpdateOptions": [
    "gomodTidy"
  ],
  "docker": {
    "reviewers": [
      "jkralik",
      "Danielius1922"
    ]
  },
  "golang": {
    "reviewers": [
      "jkralik",
      "Danielius1922"
    ]
  },
  "js": {
    "enabled": false
  },
  "commitBody": "Generated by renovateBot",
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "schedule": [
        "on the first day of the month"
      ]
    },
    {
      "matchManagers": [
        "helm-requirements",
        "helm-values",
        "helmfile",
        "helmsman",
        "helmv3",
        "kubernetes"
      ],
      "reviewers": [
        "ondrejtomcik"
      ]
    }
  ],
  "prCreation": "approval"
}